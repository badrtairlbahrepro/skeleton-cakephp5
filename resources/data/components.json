    "description": "Éléments de formulaire",
    "variants": [
      {
        "id": "text-input",
        "label": "Champ Texte",
        "code": "<div class=\"form-group\">\n  <label for=\"exampleInputEmail1\">Email</label>\n  <div class=\"input-group\">\n    <div class=\"input-group-prepend\">\n      <span class=\"input-group-text\"><i class=\"fas fa-envelope\"></i></span>\n    </div>\n    <input type=\"email\" class=\"form-control\" id=\"exampleInputEmail1\" placeholder=\"Entrez votre email\">\n  </div>\n</div>",
        "description": "Champ de saisie texte avec icône"
      },
      {
        "id": "password-input",
        "label": "Champ Mot de Passe",
        "code": "<div class=\"form-group\">\n  <label for=\"exampleInputPassword\">Mot de passe</label>\n  <div class=\"input-group\">\n    <div class=\"input-group-prepend\">\n      <span class=\"input-group-text\"><i class=\"fas fa-lock\"></i></span>\n    </div>\n    <input type=\"password\" class=\"form-control\" id=\"exampleInputPassword\" placeholder=\"Entrez votre mot de passe\">\n    <div class=\"input-group-append\">\n      <button class=\"btn btn-outline-secondary\" type=\"button\" onclick=\"togglePassword()\">\n        <i class=\"fas fa-eye\"></i>\n      </button>\n    </div>\n  </div>\n</div>\n<script>\n  function togglePassword() {\n    var input = document.getElementById('exampleInputPassword');\n    var icon = event.target;\n    if (input.type === 'password') {\n      input.type = 'text';\n      icon.className = 'fas fa-eye-slash';\n    } else {\n      input.type = 'password';\n      icon.className = 'fas fa-eye';\n    }\n  }\n</script>",
        "description": "Champ de saisie mot de passe avec icône et bouton afficher/masquer"
      },
      {
        "id": "textarea",
        "label": "Zone de Texte",
        "code": "<div class=\"form-group\">\n  <label for=\"exampleTextarea\">Message</label>\n  <div class=\"input-group\">\n    <div class=\"input-group-prepend\">\n      <span class=\"input-group-text\"><i class=\"fas fa-comment\"></i></span>\n    </div>\n    <textarea class=\"form-control\" id=\"exampleTextarea\" rows=\"4\" placeholder=\"Entrez votre message...\"></textarea>\n  </div>\n</div>",
        "description": "Zone de texte multi-lignes avec icône"
      },
      {
        "id": "file-input",
        "label": "Champ Fichier",
        "code": "<div class=\"form-group\">\n  <label for=\"exampleInputFile\">File input</label>\n  <div class=\"input-group\">\n    <div class=\"custom-file\">\n      <input type=\"file\" class=\"custom-file-input\" id=\"exampleInputFile\">\n      <label class=\"custom-file-label\" for=\"exampleInputFile\">Choose file</label>\n    </div>\n    <div class=\"input-group-append\">\n      <span class=\"input-group-text\">Upload</span>\n    </div>\n  </div>\n</div>",
        "description": "Champ de téléchargement de fichier"
      },
      {
        "id": "file-input-advanced",
        "label": "Champ Fichier Avancé",
        "code": "<div class=\"form-group\">\n  <label for=\"exampleInputFileAdvanced\">File input avec affichage</label>\n  <div class=\"input-group\">\n    <div class=\"custom-file\">\n      <input type=\"file\" class=\"custom-file-input\" id=\"exampleInputFileAdvanced\" onchange=\"updateFileName(this)\">\n      <label class=\"custom-file-label\" for=\"exampleInputFileAdvanced\" id=\"customFileLabel\">Choose file</label>\n    </div>\n    <div class=\"input-group-append\">\n      <span class=\"input-group-text\">Upload</span>\n    </div>\n  </div>\n</div>\n<script>\n  function updateFileName(input) {\n    var fileName = input.files[0] ? input.files[0].name : 'Choose file';\n    document.getElementById('customFileLabel').textContent = fileName;\n  }\n</script>",
        "description": "Champ fichier avec affichage du nom du fichier sélectionné"
      },
      {
        "id": "select",
        "label": "Sélecteur",
        "code": "<div class=\"mb-3\">\n  <label for=\"exampleSelect1\" class=\"form-label\">Sélectionner</label>\n  <select class=\"form-control\" id=\"exampleSelect1\">\n    <option>Option 1</option>\n    <option>Option 2</option>\n    <option>Option 3</option>\n  </select>\n</div>",
        "description": "Liste déroulante"
      },
      {
        "id": "checkbox",
        "label": "Case à Cocher",
        "code": "<div class=\"form-check\">\n  <input class=\"form-check-input\" type=\"checkbox\" id=\"exampleCheck1\">\n  <label class=\"form-check-label\" for=\"exampleCheck1\">\n    Cochez-moi\n  </label>\n</div>",
        "description": "Case à cocher"
      },
      {
        "id": "multiple-checkboxes",
        "label": "Plusieurs Cases à Cocher",
        "code": "<div class=\"form-check\">\n  <input class=\"form-check-input\" type=\"checkbox\" id=\"check1\">\n  <label class=\"form-check-label\" for=\"check1\">Option 1</label>\n</div>\n<div class=\"form-check\">\n  <input class=\"form-check-input\" type=\"checkbox\" id=\"check2\">\n  <label class=\"form-check-label\" for=\"check2\">Option 2</label>\n</div>\n<div class=\"form-check\">\n  <input class=\"form-check-input\" type=\"checkbox\" id=\"check3\">\n  <label class=\"form-check-label\" for=\"check3\">Option 3</label>\n</div>",
        "description": "Plusieurs cases à cocher"
      },
