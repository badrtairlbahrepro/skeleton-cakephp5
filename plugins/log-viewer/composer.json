{
    "name": "yourname/cakephp-log-viewer",
    "description": "A beautiful and feature-rich log viewer for CakePHP applications",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "plugin", "logs", "viewer", "debugging"],
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "cakephp/cakephp": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "LogViewer\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
